{
  "pages": [
    "pages/home/index",
    "pages/discover/index",
    "pages/cart/index",
    "pages/profile/index",
    "pages/cart/preview",
    "pages/profile/contributions",
    "pages/profile/settings",
    "pages/auth/login"
  ],
  "subPackages": [
    {
      "root": "pages/store",
      "pages": [
        "detail",
        "create"
      ]
    },
    {
      "root": "pages/menu",
      "pages": [
        "digitize",
        "item-detail"
      ]
    },
    {
      "root": "pages/moment",
      "pages": [
        "publish",
        "detail"
      ]
    },
    {
      "root": "pages/share",
      "pages": [
        "generate",
        "result"
      ]
    }
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "EatWhat",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#FF6B6B",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "Home",
        "iconPath": "static/icons/home.png",
        "selectedIconPath": "static/icons/home-active.png"
      },
      {
        "pagePath": "pages/discover/index",
        "text": "Discover",
        "iconPath": "static/icons/discover.png",
        "selectedIconPath": "static/icons/discover-active.png"
      },
      {
        "pagePath": "pages/cart/index",
        "text": "Cart",
        "iconPath": "static/icons/cart.png",
        "selectedIconPath": "static/icons/cart-active.png"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "Profile",
        "iconPath": "static/icons/profile.png",
        "selectedIconPath": "static/icons/profile-active.png"
      }
    ]
  },
  "preloadRule": {
    "pages/home/index": {
      "network": "all",
      "packages": [
        "pages/store"
      ]
    },
    "pages/store/detail": {
      "network": "all",
      "packages": [
        "pages/share"
      ]
    }
  },
  "lazyCodeLoading": "requiredComponents",
  "usingComponents": {}
}